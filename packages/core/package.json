{
  "name": "@vibe-stack/state-sdk",
  "version": "0.1.0",
  "description": "State management SDK with Time Travel, Branching, and DevTools for vibe coding",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zustand": "^5.0.0",
    "mutative": "^1.1.0",
    "travels": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "vitest": "^2.1.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "license": "MIT"
}
