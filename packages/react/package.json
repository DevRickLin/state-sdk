{
  "name": "@anthropic/state-sdk-react",
  "version": "0.1.0",
  "description": "React bindings and DevPanel for @anthropic/state-sdk",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic/state-sdk": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "vitest": "^2.1.0",
    "typescript": "^5.7.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "license": "MIT"
}
